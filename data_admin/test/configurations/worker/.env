# DEBUG => (True/False)
# SECRET_KEY => UUIDv4 as string
# TMP_PATH => Path for temporal files
# MODULE_NAME => Name of the module. Used on logs
# MODULE_VERSION => Version of the module. Used on logs and autoconfig
# CONFIG_NAME => (development/production)
# LOG_LEVEL => INFO default logging level
# AUTOMATIC_MIGRATIONS => migrate database before start (1 => True, 0=> False)
# BROKER_COMPONENT" => "SERVER", "WORKER_BEAT", "WORKER"

# File rotating logs
####################

# USE_FILE_ROTATING_LOG => True (1)/False (0)
# LOGS_FOLDER => Folder where logs are stored (/logs or ./logs)
# LOG_ROTATE_MAX_BYTES  => Max byte per file
# LOG_ROTATE_BACKUP_COUNT => Number of files to maintain
# FILE_ROTATING_LOG_LEVEL => Log level for file rotatin logs (default LOG_LEVEL)
# LOG_FILE => filename for the logs

# ElasticSearch logs
#######################
# USE_ELASTIC_SEARCH => True (1)/False (0)
# ES_HOST => ES host name
# ES_PORT => ES port
# ES_USER => ES user if authentication required
# ES_PASSWORD => ES password if authentication required
# ES_USE_SSL => Force the use of HTTPS to communicate with ES server
# ES_VERIFY_SSL => Verify the the certificate of the ES server (if using SSL)
# ES_INDEX_NAME => Name of the ES index to be used
# ES_LOG_LEVEL => Log level for ES logs (default LOG_LEVEL)

# STDOUT logs
###############
# USE_STDOUT => True (1)/False (0)
# STDOUT_LOG_LEVEL => Log level for StdOut logs (default LOG_LEVEL)

